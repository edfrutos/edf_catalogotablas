{
  "files.associations": {
    "*.txt": "plaintext",
    "robots.txt": "robotstxt"
  },
  "editor.detectIndentation": false,
  "editor.formatOnSave": false,
  "files.encoding": "utf8",
  "[robotstxt]": {
    "editor.wordBasedSuggestions": false,
    "editor.quickSuggestions": false,
    "editor.snippetSuggestions": "none",
    "editor.suggest.showWords": false
  }
}