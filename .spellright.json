    "tools/test_spell_check_utilities.py"
  ],
  "ignoreRegExpsByClass": {
    "markdown": [
      "/\\*[^*]*\\*+(?:[^/*][^*]*\\*+)*/",
      "<!--[\\s\\S]*?-->",
      "`[^`]*`",
      "\\[([^\\]]*)\\]\\([^)]*\\)"
    ],
    "python": [
      "\\b[A-Z]{2,}\\b",
      "\\b[A-Z][a-z]+[A-Z][a-zA-Z]*\\b",
      "\\b[a-z]+_[a-z_]+\\b",
      "\\b[A-Z_]+\\b",
      "\\b\\d+\\b",
      "\\b0x[a-fA-F0-9]+\\b"
    ]
  },
